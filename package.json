{
  "name": "typescript-conditional-exports-workaround",
  "version": "1.0.0",
  "description": "Workaround for Node.js conditional exports in TypeScript",
  "main": "dist/index.js",
  "types": "dist-types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist dist-types"
  },
  "exports": {
    ".": "./dist/index.js",
    "./exported": "./dist/exported.js"
  },
  "typesVersions": {
    "*": {
      "exported": [
        "dist-types/exported"
      ]
    }
  },
  "files": [
    "dist",
    "dist-types"
  ],
  "keywords": [],
  "author": "Teppei Sato <teppeis@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.1.3"
  }
}
